{
  "name": "discord",
  "scripts": {
    "prepare": "ts-patch install && typia patch",
    "dev": "wrangler dev src/index.ts",
    "deploy": "wrangler deploy --minify src/index.ts",
    "test": "vitest",
    "check": "biome check",
    "ci": "biome ci --reporter=github",
    "typecheck": "tsc --noEmit",
    "codegen:cf": "wrangler types",
    "codegen:typia": "typia generate --input src/typia/templates --output src/typia/generated --project tsconfig.json",
    "register": "dotenvx run -f .dev.vars -- tsx scripts/register.ts"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.14.1",
    "discord-interactions": "^4.1.0",
    "hono": "^4.6.1",
    "neverthrow": "^8.0.0",
    "typia": "^6.10.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@cloudflare/vitest-pool-workers": "^0.5.2",
    "@cloudflare/workers-types": "^4.20240909.0",
    "@types/node": "20.10.1",
    "@virtual-live-lab/tsconfig": "^2.1.5",
    "discord-api-types": "^0.37.100",
    "ts-patch": "^3.2.1",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "vitest": "^2.1.1",
    "wrangler": "^3.78.2"
  }
}
